<odoo>
    <data>
        <record id="return_order_inherit_product_category_form" model="ir.ui.view">
            <field name="name">view.inherit.product.category.form</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="account.view_category_property_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='property_account_expense_categ_id']" position="after">
                    <field name="quarantine_store_account_id"/>
                </xpath>

            </field>
        </record>
    </data>
</odoo>